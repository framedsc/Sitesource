      local addrY = getAddress("Yval")
      local addrZ = getAddress("Zval")
      writeFloat(addrX,tonumber(xCoord.Caption))
      writeFloat(addrY,tonumber(yCoord.Caption))
      writeFloat(addrZ,zVal)

      -- see Kena for example (moveMode)...
