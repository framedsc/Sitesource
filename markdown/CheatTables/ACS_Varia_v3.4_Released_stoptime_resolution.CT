       --shellExecute(cmd)
       al.getMemoryRecordByID(myPathID).Description = sPath
       if ( bCorrectProc == '9' ) then
          local nCount = 1
          -- needed to attach process to CE...
          openProcess(myGame)
          while process ~= myGame do
              openProcess(myGame)
              sleep(1000)
              nCount = nCount + 1
              if (nCount &gt; 10) then
