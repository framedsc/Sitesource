    if (bStatus == 0) then bStatus = 1; sWindowMark = '' else bStatus = 0; sWindowMark = ' ' end
    WinModemenuitem.Caption = sMenuItem2 .. sWindowMark
    writeBytes(readInteger(addrD3D),bStatus)
end -- end game windowed....

