{$Lua}

function timeStop()
local x,y = {},{1,1e-2}
x[1] = getAddress("time")
x[2] = readFloat(x[1])
x[3] = y[1]
if x[2] == y[1] then x[3] = y[2] end
writeFloat(x[1],x[3])
end

